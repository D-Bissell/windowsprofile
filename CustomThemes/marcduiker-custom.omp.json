{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "path",
          "background": "#00a2ed",
          "foreground": "#161b30",
          "properties": {
            "style": "agnoster_short"
          },
          "style": "diamond",
          "template": " \ue5ff {{ .Path }} ",
          "leading_diamond": "\ue0b6"
        },
        {
          "type": "git",
          "background": "#80fe61",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#f77622{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#e43b44{{ end }}",
            "{{ if gt .Ahead 0 }}#2ce8f5{{ end }}",
            "{{ if gt .Behind 0 }}#ffee00{{ end }}"
          ],
          "foreground": "#161b30",
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "powerline",
          "powerline_symbol": "<parentBackground,background>\ue0b0</>",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} "
        },
        {
          "type": "node",
          "background": "#6CA35E",
          "foreground": "#161b30",
          "properties": {
            "fetch_version": true
          },
          "style": "diamond",
          "leading_diamond": "<transparent,background>\ue0b0</>",
          "trailing_diamond": "\ue0b0",
          "template": " \ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} "
        },
        {
          "type": "go",
          "background": "#8ED1F7",
          "foreground": "#161b30",
          "properties": {
            "fetch_version": true
          },
          "style": "diamond",
          "leading_diamond": "<transparent,background>\ue0b0</>",
          "trailing_diamond": "\ue0b0",
          "template": " \ue626 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} "
        },
        {
          "type": "julia",
          "background": "#4063D8",
          "foreground": "#161b30",
          "properties": {
            "fetch_version": true
          },
          "style": "diamond",
          "leading_diamond": "<transparent,background>\ue0b0</>",
          "trailing_diamond": "\ue0b0",
          "template": " \ue624 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} "
        },
        {
          "type": "python",
          "background": "#FFDE57",
          "foreground": "#161b30",
          "properties": {
            "display_mode": "context"
          },
          "style": "powerline",
          "powerline_symbol": "<parentBackground,background>\ue0b0</>",
          "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }} {{ .Venv }}{{ end }} "
        },
        {
          "type": "ruby",
          "background": "#AE1401",
          "foreground": "#161b30",
          "properties": {
            "display_mode": "files",
            "fetch_version": true
          },
          "style": "powerline",
          "powerline_symbol": "<parentBackground,background>\ue0b0</>",
          "template": " \ue791 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} "
        },
        {
          "type": "azfunc",
          "background": "#FEAC19",
          "foreground": "#161b30",
          "properties": {
            "display_mode": "files",
            "fetch_version": false
          },
          "style": "powerline",
          "powerline_symbol": "<parentBackground,background>\ue0b0</>",
          "template": " \uf0e7{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} "
        },
        {
          "type": "aws",
          "background_templates": [
            "{{if contains \"default\" .Profile}}#FFA400{{end}}",
            "{{if contains \"jan\" .Profile}}#f1184c{{end}}"
          ],
          "foreground": "#161b30",
          "properties": {
          },
          "style": "powerline",
          "powerline_symbol": "<parentBackground,background>\ue0b0</>",
          "template": ""
        },
        {
          "type": "status",
          "background": "#00a2ed",
          "background_templates": [
            "{{ if gt .Code 0 }}#ff0044{{ end }}"
          ],
          "foreground": "#161b30",
          "properties": {
            "always_enabled": true
          },
          "style": "diamond",
          "trailing_diamond": "\ue0b4",
          "template": "{{ if gt .Code 0 }} \ue009{{ reason .Code }}{{ end }}{{ if .Root }}<#FFDE57> \uf0e7</>{{ else }} \ue691{{ end }} "
        }
      ]
    },
    {
      "type": "rprompt",
      "alignment": "right",
      "segments": [
        {
          "type": "az",
          "background": "#0077c2",
          "foreground": "#ffffff",
          "properties": {
            "source": "cli"
          },
          "style": "diamond",
          "leading_diamond": "<background,transparent>\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\uebd8 {{ if gt (len .Name) 30 }}{{ .Name | substr 0 10 }}...{{ .Name | substr (sub (len .Name) 5) 5 }}{{ else }}{{ .Name }}{{ end }}</>"
        },
        {
          "type": "az",
          "background": "#0077c2",
          "foreground": "#ffffff",
          "properties": {
            "source": "pwsh"
          },
          "style": "diamond",
          "leading_diamond": "<background,transparent>\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\uebd8 {{ if gt (len .Name) 30 }}{{ .Name | substr 0 10 }}...{{ .Name | substr (sub (len .Name) 5) 5 }}{{ else }}{{ .Name }}{{ end }}</>"
        }
      ]
    }
  ],
  "final_space": true,
  "version": 2
}
